{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "exclude": ["node_modules"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_|(node_modules)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "destination": "./doc/",
        "encoding": "utf8",
        "private": "true",
        "recurse": true
    }
}
